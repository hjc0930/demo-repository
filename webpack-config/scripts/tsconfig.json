{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext"
    ],
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    /** Strict */
    "strict": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "composite": true,
    /** Module */
    "isolatedModules": true,
    "moduleResolution": "Bundler"
  },
  "include": [
    "."
  ]
}